<form action="Form1ProyectoServlet" class="needs-validation">

	<div class="form-group">
		<Label for="title">Nombre proyecto</Label>
		<input type="text" name="title" placeholder="Titulo" />
	</div>
	<div class="form-group">
		<Label for="numeroTrabajadores">Nº Trabajadores</Label>
		<input type="number" name="numeroTrabajadores" placeholder="Numero Trabajadores"/>
